{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:hi@mintlify.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.mintlify.com"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://mintlify.com/blog"
    }
  ],
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "introduction",
          "quickstart",
          "development"
        ]
      },
      {
        "group": "Essentials",
        "pages": [
          "essentials/markdown",
          "essentials/code",
          "essentials/images",
          "essentials/settings",
          "essentials/navigation",
          "essentials/reusable-snippets"
        ]
      },
      {
        "group": "API Documentation",
        "pages": [
          "api-reference/introduction"
        ]
      },
      {
        "group": "Purchase Orders API",
        "pages": [
          "api-reference/purchase-orders/microservice-architecture",
          "api-reference/purchase-orders/direct-redemption-flow",
          "api-reference/purchase-orders/shopping-cart-flow",
          "api-reference/purchase-orders/payloads-reference",
          "api-reference/purchase-orders/database-tables",
          "api-reference/purchase-orders/error-handling",
          "api-reference/purchase-orders/add-to-cart",
          "api-reference/purchase-orders/remove-from-cart",
          "api-reference/purchase-orders/get-cart",
          "api-reference/purchase-orders/purchase-direct",
          "api-reference/purchase-orders/purchase-cart"
        ]
      },
      {
        "group": "Endpoint Examples",
        "pages": [
          "api-reference/endpoint/get",
          "api-reference/endpoint/create",
          "api-reference/endpoint/delete",
          "api-reference/endpoint/webhook"
        ]
      }
    ]
  },
  "api": {
    "baseUrl": "https://api.example.com/api/v2",
    "auth": {
      "method": "bearer"
    },
    "playground": {
      "mode": "show"
    }
  },
  "openapi": [
    "api-reference/openapi.json",
    "api-reference/purchase-orders-openapi.json"
  ],
  "footerSocials": {
    "x": "https://x.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintlify"
  },
  "theme": "mint"
}
